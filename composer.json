{
    "name": "derralf/elemental-archive-report",
    "description": "Simple experimantal extension to list (accidentally) archived Elements and to unarchive them",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "dnadesign/silverstripe-elemental": "3.0.x-dev",
        "silverstripe/cms": "^4.2",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Derralf\\Elemental\\ArchiveReport\\": "src/"

        }
    }
}

